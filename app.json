{
  "expo": {
    "name": "MapNShop",
    "slug": "mapnshop",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/new-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mrlaksaci.mapnshop",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/new-icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-location",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to add attachments to orders.",
          "cameraPermission": "The app accesses your camera to add photo attachments to orders."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "7645303c-3af8-46c4-9355-3e27539a92ae"
      },
      "router": {}
    },
    "android": {
      "package": "com.mrlaksaci.mapnshop",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Logo2.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "splash": {
      "image": "./assets/images/new-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "owner": "mapnshop"
  }
}